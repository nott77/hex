if global.devmode == true {
    draw_set_halign(fa_center);
    draw_set_valign(fa_middle);
    draw_set_color(c_black);
    
    // Grid x,y position
    draw_text(x, y-35, string(grid_x) + " , " + string(grid_y));
    
    // World x,y position
